  <!-- Why Choose Astro Section -->
  <section class="bg-body py-5">
    <div class="container">
      <h2 class="text-center mb-2" data-aos="fade-up" data-aos-duration="1000">
        Why to Choose Astero Admin Bootstrap Template?
