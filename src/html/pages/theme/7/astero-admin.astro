  <!-- Dark Mode Section -->
  <section class="bg-body py-5">
    <div class="container">
      <div class="row align-items-center g-4">
        <div class="col-lg-6">
          <div
            class="d-flex align-items-center mb-3"
            data-aos="fade-in"
