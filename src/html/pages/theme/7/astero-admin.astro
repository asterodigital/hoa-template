---
import Layout from './layout.astro'
// import { getPathPrefix } from '../../../../utils/path.js'
---

<Layout title="Astero Admin" description="ROI-driven Digital Marketing Company.">
  <!-- Heading Section -->
  <section class="bg-body py-5">
    <div class="container">
      <div class="row">
        <div class="col-12 col-lg-8 offset-lg-2 text-center">
          <div
            class="d-flex align-items-center justify-content-center mb-3"
            data-aos="fade-in"
            data-aos-delay="100"
            data-aos-duration="1000"
          >
            <div
              class="d-flex align-items-center bg-primary-soft border border-primary rounded-pill px-3 py-2"
            >
              <i class="ri-flashlight-line text-primary fs-5 me-1"></i>
              <span class="text-primary">New Release 2025</span>
            </div>
          </div>
          <h1 class="fs-1 mb-3" data-aos="fade-up" data-aos-duration="1000">
            Astero Admin - Free Bootstrap 5 Admin Template
          </h1>
          <p
            class="fs-5 text-muted mb-3"
            data-aos="fade-up"
            data-aos-delay="100"
            data-aos-duration="1000"
          >
            Transform your projects with a modern, feature-rich dashboard template.
          </p>
          <a
            href="https://astero-admin.asterodigital.com/pages/dashboard"
            target="_blank"
            class="btn btn-lg btn-primary px-3 py-1 me-2"
            data-aos="fade-up"
            data-aos-delay="150"
            data-aos-duration="1000"
          >
            Demo<i class="ri-arrow-right-line ms-1"></i>
          </a>
          <a
            href="https://github.com/asterodigital/bootstrap-admin-template"
            target="_blank"
            class="btn btn-lg btn-outline-primary px-3 py-1 me-2"
            data-aos="fade-up"
            data-aos-delay="200"
            data-aos-duration="1000"
          >
            <i class="ri-github-line fs-4 me-1"></i>GitHub
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- Landing Page -->
  <section
    class="bg-body py-2 py-sm-3 py-md-4 py-lg-5"
    style="background-image: url('https://asterodigital.com/storage/local/aalp/cms/media/99/gradient.png'); background-size: cover; background-position: center; position: relative;"
    data-aos="fade-in"
    data-aos-delay="200"
    data-aos-duration="1000"
  >
    <div class="row justify-content-center">
      <div class="col-12 col-md-10 col-lg-8 position-relative">
        <div class="text-center">
          <img
            src="https://asterodigital.com/storage/local/aamf/cms/media/124/dashboard-img-2.png"
            class="img-fluid rounded-4 border"
            alt="Hero Image"
          />
        </div>
      </div>
    </div>
  </section>

  <!-- Tech Stack Section -->
  <section class="bg-body py-5">
    <div class="container">
      <div class="row row-cols-2 row-cols-md-3 row-cols-xl-5 gx-lg-5 gy-4 justify-content-center">
        <div class="col">
          <div
            class="card align-items-center border-0 h-100 py-0"
            data-aos="zoom-in"
            data-aos-duration="1000"
          >
            <div class="card-body d-flex align-items-center">
              <figure class="mb-0">
                <img
                  src="https://asterodigital.com/storage/local/aalp/cms/media/96/bootstrap-logo.svg"
                  alt="Bootstrap"
                  style="max-width: 70px;"
                />
              </figure>
            </div>
          </div>
        </div>
        <div class="col">
          <div
            class="card align-items-center border-0 h-100 py-0"
            data-aos="zoom-in"
            data-aos-delay="100"
            data-aos-duration="1000"
          >
            <div class="card-body d-flex align-items-center">
              <figure class="mb-0">
                <img
                  src="https://asterodigital.com/storage/local/aalp/cms/media/107/Npm-logo.svg"
                  alt="NPM Logo"
                  style="max-width: 90px;"
                />
              </figure>
            </div>
          </div>
        </div>

        <div class="col">
          <div
            class="card align-items-center border-0 h-100 py-0"
            data-aos="zoom-in"
            data-aos-delay="200"
            data-aos-duration="1000"
          >
            <div class="card-body d-flex align-items-center">
              <figure class="mb-0">
                <img
                  src="https://asterodigital.com/storage/local/aalp/cms/media/108/sass.png"
                  alt="SASS Logo"
                  style="max-width: 90px;"
                />
              </figure>
            </div>
          </div>
        </div>
        <div class="col">
          <div
            class="card align-items-center border-0 h-100 py-0"
            data-aos="zoom-in"
            data-aos-delay="300"
            data-aos-duration="1000"
          >
            <div class="card-body d-flex align-items-center">
              <figure class="mb-0">
                <img
                  src="https://asterodigital.com/storage/local/aalp/cms/media/109/astro-icon-dark.png"
                  alt="Astro Logo"
                  style="max-width: 60px;"
                />
              </figure>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Features -->
  <section class="bg-body py-5">
    <div class="container">
      <div class="row mb-4">
        <div class="col-lg-6">
          <div
            class="d-flex align-items-center mb-3"
            data-aos="fade-in"
            data-aos-delay="200"
            data-aos-duration="1000"
          >
            <div
              class="d-flex align-items-center bg-primary-soft border border-primary rounded-pill px-3 py-2"
            >
              <i class="ri-shield-check-line text-primary fs-5 me-1"></i>
              <span class="text-primary">Features</span>
            </div>
          </div>
          <div>
            <h2 class="mb-2" data-aos="fade-down" data-aos-delay="100" data-aos-duration="1000">
              Essential features for Efficient Dashboard Management
            </h2>
          </div>
          <div>
            <p class="fs-5 text-muted mb-0" data-aos="fade-down" data-aos-duration="1000">
              Experience intuitive features that simplify management and enhance your workflow.
            </p>
          </div>
        </div>
      </div>
      <div class="row g-4">
        <div class="col-12 col-md-6 col-lg-4">
          <div class="card card-lift border h-100" data-aos="fade-in" data-aos-duration="1000">
            <div class="card-body d-flex">
